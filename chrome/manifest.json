{
    "manifest_version": 2,
    "name" : "ViewAll",
    "version": "1",

    "description": "A detector for biases",
    "browser_action": {
	"default_icon": "logo1.png",
	"default_popup": "popup.html"
	},
    "permissions" : [
	"activeTab"
	],
    "content_scripts" : [
	{
	    "matches":["<all_urls>"],
	    "js":["jquery-2.1.4.min.js", "content.js"]
	}
    ]
}
